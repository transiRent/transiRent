<h2>Offers</h2>
<div>
  {{#each offers}}
  <div>
    <h3><a href="/offers/{{this._id}}">{{this.name}}</a></h3>
    <p>{{this.type}}</p>
    <p>{{this.address.street}} {{this.address.number}}, {{this.address.code}} {{this.address.city}}</p>
  </div>
  {{/each}}
</div>
<a href="/offers/create">Create Offer</a>